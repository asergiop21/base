<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>
<div class="container">
        <div class="span12">
            <div class="span6">
              <%= f.input :email, :autofocus => true %>
              <%= f.input :username %>
              <%= f.input :name %>
              <%= f.input :lastname %>
              <%= f.input :address %>

            </div>
            <div class ="span5">
                    
              <%= f.input :cuit %>
              <%= f.input :dni %>
                    
              <%= f.select :role, User::ROLE  %>
              <%= f.input :password %>
              <%= f.input :password_confirmation%>
              <%= f.input :enabled %>
            </div>
        <div class="span12"><%= f.button :submit %></div>
</div>
<% end %>
