<h1>Grupos de Etiquetas</h1>

<table class="table">
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @group_labels.each do |group_label| %>
  <tr>
    <td><%= group_label.name %></td>
    <td><%= link_to 'Imprimir', group_label_path(group_label, format: 'pdf') %></td>
    <td><%= link_to 'Edit', edit_group_label_path(group_label) %></td>
    <td><%= link_to 'Destroy', group_label, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Grupo', new_group_label_path %>
